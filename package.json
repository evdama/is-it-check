{
  "name": "is_js",
  "version": "0.9.1",
  "main": "is.js",
  "license": "MIT",
  "description": "micro check library",
  "homepage": "https://github.com/evdama/is.js",
  "repository": "evdama/is.js",
  "scripts": {
    "build": "npm run lint && npm run min",
    "lint": "eslint .",
    "min": "uglifyjs is.js -m --comments \"/^!/\" -o is.min.js",
    "test": "mocha --check-leaks -R dot",
    "test:phantom": "mocha-phantomjs -R dot test/index.html"
  },
  "pre-commit": [
    "lint"
  ],
  "devDependencies": {
    "chai": "^4.3.6",
    "eslint": "^8.11.0",
    "lodash": "^4.17.21",
    "mocha": "^9.2.2",
    "mocha-phantomjs": "^4.1.0",
    "pre-commit": "^1.2.2",
    "uglify-js": "^3.15.3"
  },
  "files": [
    "is.js",
    "is.min.js"
  ]
}
